pipeline {
	agent none
  	triggers {
  		cron '* * 15 * *'
	upstream 'demo-pipeline1, '
  	}

	stages {
		stage('stage-1') {
			agent {label 'master'}
			steps {
				sh 'sleep 60'
				echo 'sh mkdir master-1'
			}
		}

		stage('stage-2') {
			agent {label 'java_node'}
			steps {
				sh 'sleep 60'
				echo 'sh mkdir java_node_1'
			}
		}
	}
}
